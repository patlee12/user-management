{
  "name": "user-management-backend",
  "version": "1.0.0",
  "description": "A Nest.js application for backend user management related features.",
  "author": "Patrick Lee",
  "private": true,
  "license": "MIT",
  "scripts": {
    "generate:env": "ts-node -P tsconfig.json scripts/generate-backend-env.ts",
    "generate:types": "curl -k https://localhost:3001/api-json -o ../../libs/types/swagger.json && NODE_TLS_REJECT_UNAUTHORIZED=0 npx openapi-typescript-codegen --input ../../libs/types/swagger.json --output ../../libs/types/src/generated && rm ../../libs/types/swagger.json",
    "build": "yarn prisma generate && nest build --path tsconfig.build.json",
    "copy-mailing-templates": "mkdir -p dist/src/user-management-app/mailing/templates && cp -R src/user-management-app/mailing/templates/. dist/src/user-management-app/mailing/templates",
    "start": "nest start",
    "start:dev": "yarn prisma generate && yarn prisma migrate dev && yarn seed:admin:dev && yarn build && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:prod:env": "dotenv -e .env -- node dist/src/main",
    "localareanetwork:prod": "dotenv -e .env,.env.localareanetwork -- yarn bootstrap:prod",
    "bootstrap:prod": "yarn migrate && yarn seed:admin && yarn start:prod",
    "migrate": "yarn prisma migrate deploy",
    "migrate:reset": "yarn prisma migrate reset --force --skip-seed",
    "seed:admin": "node dist/scripts/run-admin-seed.js",
    "seed:admin:dev": "ts-node --cwd ./ -P tsconfig.json scripts/run-admin-seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "prisma": {
    "seed": "node dist/prisma/seed.js"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/prisma": "^5.0.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.3.1",
    "adminjs": "^7.8.15",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.0.0",
    "prisma": "^6.3.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "selfsigned": "^2.4.1",
    "source-map-support": "^0.5.21",
    "speakeasy": "^2.0.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "openapi-typescript": "^7.6.1",
    "openapi-typescript-codegen": "^0.29.0"
  }
}
